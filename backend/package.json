    "@fastify/static": "^7.0.1",
    "@prisma/client": "^5.8.1",
    "fastify": "^4.25.2",
    "openai": "^4.24.1",
    "zod": "^3.22.4",
    "dotenv": "^16.3.1",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "nanoid": "^5.0.4",
    "bcrypt": "^5.1.1",
    "date-fns": "^3.0.6"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
